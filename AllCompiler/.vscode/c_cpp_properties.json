{
  "configurations": [
    {
      "name": "Linux G++",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/Front/Compiler",
        "${workspaceFolder}/Front/Parser",
        "${workspaceFolder}/Front/Token",
        "${workspaceFolder}/Struct/NameTable",
        "${workspaceFolder}/Struct/Vector",
        "${workspaceFolder}/Sup/Common",
        "${workspaceFolder}/Sup/Configs",
        "${workspaceFolder}/Sup/DumpCode",
        "${workspaceFolder}/Sup/Dump",
        "${workspaceFolder}/Sup/Support",
        "/usr/include",
        "/usr/local/include"
      ],
      "defines": [
        "_DEBUG"
      ],
      "compilerPath": "/usr/bin/g++",
      "cStandard": "gnu17",
      "cppStandard": "gnu++17",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerArgs": [
        "-std=c++17",
        "-D_DEBUG",
        "-ggdb3",
        "-I${workspaceFolder}/**",
        "-I${workspaceFolder}/Front/Compiler",
        "-I${workspaceFolder}/Front/Parser",
        "-I${workspaceFolder}/Front/Token",
        "-I${workspaceFolder}/Struct/NameTable",
        "-I${workspaceFolder}/Struct/Vector",
        "-I${workspaceFolder}/Sup/Common",
        "-I${workspaceFolder}/Sup/Configs",
        "-I${workspaceFolder}/Sup/DumpCode",
        "-I${workspaceFolder}/Sup/Dump",
        "-I${workspaceFolder}/Sup/Support"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}/**"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
      }
    }
  ],
  "version": 4
}
